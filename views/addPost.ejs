<h1><%- title %></h1>
<form id="addPostForm" method="post">
    <div class="fieldset">
        <label for="data_path">Post</label>
        <input type="text" name="data_path" id="post" required>
    </div>
    <div class="fieldset">
        <label for="description">Description</label>
        <input type="text" name="description" id="description">
    </div>
    <div class="fieldset">
        <input type="submit" value="Add post">
    </div>
</form>